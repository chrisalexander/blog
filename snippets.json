		],
		"description": "Gist"
	},
	"Front matter": {
		"prefix": "frontmatter",
		"body": [
			"---",
			"layout: post",
			"heading: '${2:heading}'",
			"subheading: '${3:subheading}'",
			"leader:",
			"	image: ${4:image}",
			"	hassmall: false",
			"	showonpage: false",
			"	inverted: false",
			"categories: ${5:category}",
			"---",
			"",
			"$0"
		]
	}
